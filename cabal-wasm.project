with-compiler: wasm32-wasi-ghc-9.10.0.20240412
import: cabal-common.project
packages: cloudflare-kv-manager/*.cabal
optimization: False
active-repositories: hackage.haskell.org
allow-newer:
  base,
  bytestring,
  containers,
  deepseq,
  filepath,
  ghc-prim,
  text,
  time,

package QuickCheck
  flags: -templateHaskell

package reflection
  flags: -template-haskell
